  object qryFornecedor: TZQuery
    Connection = dmConexao.dbCon
    UpdateObject = uqryFornecedor
    SQL.Strings = (
      'select f.*, c.cidade, c.uf'
      'from fornecedor f, cidade c'
      'where f.cidade_codcidade = c.codcidade'
    )
    Params = <>
    Left = 872
    Top = 664
  end
  object uqryFornecedor: TZUpdateSQL
    DeleteSQL.Strings = (
      'DELETE FROM fornecedor'
      'WHERE'
      '  fornecedor.codfornecedor = :OLD_codfornecedor'
    )
    InsertSQL.Strings = (
      'INSERT INTO fornecedor'
      '  (codfornecedor, cidade_codcidade, fornecedor, endereco, num, bairro, '
      '   cep, contato, telefone, insc, cnpj)'
      'VALUES'
      '  (:codfornecedor, :cidade_codcidade, :fornecedor, :endereco, :num, :bairro, '
      '   :cep, :contato, :telefone, :insc, :cnpj)'
    )
    ModifySQL.Strings = (
      'UPDATE fornecedor SET'
      '  codfornecedor = :codfornecedor,'
      '  cidade_codcidade = :cidade_codcidade,'
      '  fornecedor = :fornecedor,'
