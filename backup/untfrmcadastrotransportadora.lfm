  object qryTransportadora: TZQuery
    Connection = dmConexao.dbCon
    UpdateObject = uqryTransportadora
    SQL.Strings = (
      'select t.*, c.cidade, c.uf'
      'from transportadora t, cidade c'
      'where t.cidade_codcidade = c.codcidade'
    )
    Params = <>
    Left = 842
    Top = 634
  end
  object uqryTransportadora: TZUpdateSQL
    DeleteSQL.Strings = (
      'DELETE FROM transportadora'
      'WHERE'
      ' ((transportadora.codtransportadora IS NULL AND :OLD_codtransportadora IS NULL) OR (transportadora.codtransportadora = :OLD_codtransportadora))'
      ''
    )
    InsertSQL.Strings = (
      'INSERT INTO transportadora'
      '  (codtransportadora, cidade_codcidade, transportadora, endereco, num, '
      '   bairro, cep, contato, telefone, insc, cnpj)'
      'VALUES'
      '  (:codtransportadora, :cidade_codcidade, :transportadora, :endereco, :num, '
      '   :bairro, :cep, :contato, :telefone, :insc, :cnpj)'
    )
    ModifySQL.Strings = (
      'UPDATE transportadora SET'
      '  codtransportadora = :codtransportadora,'
      '  cidade_codcidade = :cidade_codcidade,'
      '  transportadora = :transportadora,'
      '  endereco = :endereco,'
      '  num = :num,'
      '  bairro = :bairro,'
      '  cep = :cep,'
      '  contato = :contato,'
      '  telefone = :telefone,'
      '  insc = :insc,'
      '  cnpj = :cnpj'
      'WHERE ((transportadora.codtransportadora IS NULL AND :OLD_codtransportadora IS NULL) OR (transportadora.codtransportadora = :OLD_codtransportadora))'
    )
